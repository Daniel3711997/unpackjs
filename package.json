{
    "private": true,
    "name": "unpackjs",
    "version": "1.0.1",
    "description": "Lorem Ipsum is simply dummy text of the printing and typesetting industry",
    "scripts": {
        "release": "release-it",
        "prepare": "husky install",
        "build-application": "encore production",
        "clear": "node ./scripts/_clearWorkspace.js",
        "update-db": "npx update-browserslist-db@latest",
        "test": "jest --passWithNoTests",
        "test-watch": "jest --passWithNoTests --watch",
        "check-php": "./vendor/bin/phpcs app --standard=.php-style.xml --cache",
        "test-php": "php ./vendor/bin/phpunit __tests__/php --testdox --cache-result-file \"cache/tools/phpunit\" --cache-result",
        "cspell": "cspell --no-progress \"{app,src}/**/*.{php,js,jsx,ts,tsx,css,scss}\" --cache --cache-strategy metadata --cache-location \"cache/tools/cspell\"",
        "eslint": "eslint --cache --cache-strategy metadata --cache-location \"cache/eslint/\" \"src/**/*.{js,jsx,ts,tsx}\"",
        "stylelint": "stylelint --cache --cache-strategy metadata --cache-location \"cache/stylelint/\" \"src/**/*.{css,scss}\"",
        "prebuild": "npm run cspell && npm run check-php",
        "build": "tsc && npm run build-application",
        "postbuild": "composer dump-autoload --optimize && node ./scripts/_postbuild.js",
        "build-action": "tsc && npm run build-application",
        "postbuild-action": "node ./scripts/_postbuild.js",
        "prestart": "composer dump-autoload",
        "start": "concurrently -r -k \"typed-scss-modules \\\"**/*.module.scss\\\" -L silent -n none -e default -w -o .generated\" \"encore dev-server\"",
        "configure-development": "npm install && composer install",
        "postconfigure-development": "node ./scripts/_postconfigure.js",
        "configure-production": "npm ci && composer install --no-interaction --optimize-autoloader",
        "postconfigure-production": "node ./scripts/_postconfigure.js",
        "enable-swc": "cross-env SWC=true node ./scripts/_SWCAppLoader.js",
        "disable-swc": "cross-env SWC=false node ./scripts/_SWCAppLoader.js",
        "check": "npm run cspell && npm run check-php && npm run stylelint && npm run eslint && npm run test && npm run test-php"
    },
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "lint-staged": {
        "**/*.php": [
            "./vendor/bin/phpcs --standard=.php-style.xml --cache"
        ],
        "**/*.{js,jsx,ts,tsx}": [
            "eslint --cache --cache-strategy metadata --cache-location \"cache/eslint/\""
        ],
        "**/*.{css,scss}": [
            "stylelint --cache --cache-strategy metadata --cache-location \"cache/stylelint/\""
        ],
        "**/*.{js,jsx,ts,tsx,json,css,scss}": [
            "prettier --write --cache --cache-strategy metadata --cache-location \"cache/tools/prettier\""
        ],
        "{app,src}/**/*.{php,js,jsx,ts,tsx,css,scss}": [
            "cspell --no-progress --cache --cache-strategy metadata --cache-location \"cache/tools/cspell\""
        ]
    },
    "browserslist": {
        "production": [
            "chrome 64",
            "edge 79",
            "firefox 67",
            "opera 51",
            "safari 12"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version"
        ]
    },
    "engines": {
        "npm": ">=9.0.0",
        "node": ">=18.0.0"
    },
    "includeNodeModules": [],
    "overrides": {
        "css-modules-loader-core": {
            "postcss": "^7.0.0"
        },
        "@symfony/webpack-encore": {
            "fork-ts-checker-webpack-plugin": {
                ".": "^8.0.0",
                "cosmiconfig": {
                    "yaml": "^2.0.0"
                }
            },
            "css-minimizer-webpack-plugin": "^5.0.0"
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.22.3",
        "@loadable/component": "^5.15.3",
        "@lukemorales/query-key-factory": "^1.2.0",
        "@swc/helpers": "^0.5.1",
        "@tanstack/react-query": "^4.29.3",
        "@tanstack/react-query-devtools": "^4.29.12",
        "axios": "^1.4.0",
        "clsx": "^1.2.1",
        "final-form": "^4.20.9",
        "immer": "^10.0.2",
        "jquery": "^3.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.9",
        "react-final-form": "^6.5.9",
        "tslib": "^2.5.3",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.22.4",
        "@babel/preset-react": "^7.22.3",
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@fullhuman/postcss-purgecss": "^5.0.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@release-it/conventional-changelog": "^5.1.1",
        "@swc/core": "^1.3.62",
        "@swc/jest": "^0.2.26",
        "@swc/plugin-transform-imports": "^1.5.67",
        "@symfony/webpack-encore": "^4.3.0",
        "@tanstack/eslint-plugin-query": "^4.29.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.2",
        "@types/jquery": "^3.5.16",
        "@types/loadable__component": "^5.13.4",
        "@types/node": "^20.2.5",
        "@types/react": "^18.2.8",
        "@types/react-dom": "^18.2.4",
        "@types/webpack-env": "^1.18.1",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "babel-plugin-transform-imports": "^2.0.0",
        "chalk": "^4.1.2",
        "compression-webpack-plugin": "^10.0.0",
        "concurrently": "^8.1.0",
        "core-js": "^3.30.2",
        "cross-env": "^7.0.3",
        "cspell": "^6.31.1",
        "dotenv-webpack": "^8.0.1",
        "esbuild": "^0.17.19",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-sort-exports": "^0.8.0",
        "eslint-webpack-plugin": "^4.0.1",
        "fork-ts-checker-webpack-plugin": "^8.0.0",
        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "lint-staged": "^13.2.2",
        "node-ssh": "^13.1.0",
        "postcss": "^8.4.24",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^7.3.2",
        "postcss-preset-env": "^8.4.2",
        "prettier": "^2.8.8",
        "purgecss-with-wordpress": "^6.0.0-alpha.0",
        "react-refresh": "^0.14.0",
        "release-it": "^15.10.5",
        "sass": "^1.62.1",
        "sass-loader": "^13.3.1",
        "stylelint": "^15.6.3",
        "stylelint-config-clean-order": "^5.0.1",
        "stylelint-config-sass-guidelines": "^10.0.0",
        "stylelint-webpack-plugin": "^4.1.1",
        "swc-loader": "^0.2.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "tsconfig-paths-webpack-plugin": "^4.0.1",
        "typed-scss-modules": "^7.1.0",
        "typescript": "^5.1.3",
        "webpack-bundle-analyzer": "^4.9.0",
        "webpackbar": "^5.0.2"
    }
}
