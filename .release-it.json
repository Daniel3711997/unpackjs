{
    "github": {
        "release": true
    },
    "npm": {
        "publish": false
    },
    "plugins": {
        "@release-it/conventional-changelog": {
            "preset": "angular",
            "infile": "CHANGELOG.md"
        }
    },
    "git": {
        "tagName": "v${version}",
        "commitMessage": "chore: release ${version}"
    }
}
